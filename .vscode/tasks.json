{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "docker-build",
			"label": "docker-build",
			"platform": "python",
			"dockerBuild": {
				"tag": "youtubeautoarchive:latest",
				"dockerfile": "${workspaceFolder}/Dockerfile",
				"context": "${workspaceFolder}",
				"pull": true
			}
		},
		{
			"type": "docker-run",
			"label": "docker-run: debug",
			"dependsOn": [
				"docker-build"
			],
			"python": {
				"file": "youtube-auto-archive.py"
			},
			"dockerRun": {
				"env": {
					"SLEEPMIN": "1",
					"TODOWNLOAD": "https://www.youtube.com/watch?v=wJQmfYsFk4w&list=LL&index=2"
				},
				"volumes": [{
					"containerPath": "/output",
					"localPath": "C:\\Users\\Wolf\\Desktop\\tempmount",
					"permissions": "rw"
				}]
			}
		}
	]
}